{
  "author": "sangdth",
  "description": "Strong opinionated expo template",
  "license": "MIT",
  "homepage": "https://github.com/sangdth/opinionated-expo-template",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "dev": "yarn cache clean && expo start --ios --clear",
    "lint": "eslint --quiet -c .eslintrc './**/*.{ts,js,tsx,jsx}'",
    "ts-check": "node --max_old_space_size=4096 node_modules/.bin/tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "test:ci": "yarn test --ci --maxWorkers=50% --silent",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject"
  },
  "dependencies": {
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-native-picker/picker": "1.16.1",
    "@react-navigation/native": "^6.0.2",
    "expo": "^42.0.3",
    "expo-status-bar": "~1.0.4",
    "jotai": "^1.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^2.2.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.4.0",
    "react-native-ui-lib": "^5.28.4",
    "react-native-web": "~0.13.12",
    "react-use": "^17.2.4"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@types/react": "17.0.19",
    "@types/react-native": "0.64.13",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-typescript": "14.0.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "typescript": "4.4.2"
  }
}
